# Path to your oh-my-zsh installation.
export ZSH="/home/sganon/.oh-my-zsh"
ZSH_THEME="amuse"

plugins=(git docker arch)

source $ZSH/oh-my-zsh.sh

# Go Setup
export GOPATH=$HOME/go
export GO111MODULE=on

# Aliases
alias vim="nvim"
alias bbvault='eval $($HOME/config/secrets/bb_vault.sh set)'
alias gltoken='eval $($HOME/config/secrets/gltoken)'

